        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasTime"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#TemporalEntity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Istant"/>
