        <rdfs:comment xml:lang="en">Beginning of a temporal entity.</rdfs:comment>
        <rdfs:comment xml:lang="it">Inizio di un&apos;entità temporale.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasCreator -->

