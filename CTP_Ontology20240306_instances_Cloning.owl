    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#lettre_e_M._l&apos;Abbé_Fontana_à_M._le_Duc_de_Chaulnes">
        <rdf:type rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#CulturalResource"/>
        <hasRelationToAgent rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Author_1"/>
