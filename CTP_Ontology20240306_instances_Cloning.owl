    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/Agent -->

    <owl:Class rdf:about="http://purl.org/dc/terms/Agent">
        <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </owl:Class>
    


    <!-- http://purl.org/dc/terms/Location -->

    <owl:Class rdf:about="http://purl.org/dc/terms/Location">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Place"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/dc/terms/LocationPeriodOrJurisdiction"/>
        <rdfs:comment xml:lang="en">A spatial region or named place.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
        <rdfs:label xml:lang="en">Location</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/dc/terms/LocationPeriodOrJurisdiction -->

    <owl:Class rdf:about="http://purl.org/dc/terms/LocationPeriodOrJurisdiction"/>
    


    <!-- http://purl.org/ontology/olo/core#OrderedList -->

    <owl:Class rdf:about="http://purl.org/ontology/olo/core#OrderedList"/>
    


    <!-- http://purl.org/ontology/olo/core#Slot -->

    <owl:Class rdf:about="http://purl.org/ontology/olo/core#Slot">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#OrderedListSlot"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/olo/core#next"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/olo/core#ordered_list"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/olo/core#previous"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/olo/core#index"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A slot in an ordered list with a fixed index.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/olo/core#"/>
        <rdfs:label xml:lang="en">Slot</rdfs:label>
        <ns:term_status xml:lang="en">stable</ns:term_status>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Agent -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Agent">
        <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">A person, family, group or institution that performs a certain action.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una persona, famiglia, gruppo o istituzione che compie una certa azione.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#CulturalEvent -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#CulturalEvent">
        <rdfs:comment xml:lang="en">A planned or an unplanned cultural situation that includes a number of activities. It occurs in a certain time and space, and may involve agents.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una situazione culturale pianificata o non pianificata che comprende una serie di attività. Si verifica in un determinato tempo e spazio e può coinvolgere agenti.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#CulturalResource -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#CulturalResource">
        <rdfs:comment xml:lang="en">A tangible or intangible resource, movable or immovable, realized by nature or man-made, that has been identified as possessing artistic, historical, archaeological, or ethno-anthropological significance.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una risorsa tangibile o intangibile, mobile o immobile, realizzata dalla natura o dall&apos;uomo, identificata come possedente significato artistico, storico, archeologico o etno-antropologico.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Group -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Group">
        <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Group"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#GroupsOfPeople"/>
        <rdfs:comment xml:lang="en">A collection of individual agents that can itself act as an agent, capable of performing actions.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una collezione di agenti individuali che può a sua volta agire come un agente, capace di compiere azioni.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#GroupsOfPeople -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#GroupsOfPeople">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Agent"/>
        <rdfs:comment xml:lang="en">A collection of individual agents.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una collezione di agenti individuali.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Identifier -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Identifier">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/adms#Identifier"/>
        <rdfs:comment xml:lang="en">A sequence of characters or symbols that serves to uniquely distinguish an entity or object within a specific system or context.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una sequenza di caratteri o simboli che serve a distinguere in modo univoco un&apos;entità o un oggetto all&apos;interno di un sistema o contesto specifico.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Interval -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Interval">
        <owl:equivalentClass rdf:resource="http://www.w3.org/2006/time#Interval"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#TemporalEntity"/>
        <rdfs:comment xml:lang="en">A temporal entity with an extent or duration.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un&apos;entità temporale con un&apos;estensione o una durata.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Istant -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Istant">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#TemporalEntity"/>
        <rdfs:comment xml:lang="en">A temporal entity with zero extent or duration.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un&apos;entità temporale con durata o estensione pari a zero.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#OrderedListSlot -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#OrderedListSlot">
        <rdfs:comment xml:lang="en">A thematic path is structured as a collector of cultural objects or present with a casual or predetermined order. In the latter case, cultural objects can be sorted according to a certain criterion (i.e. alphabetical, chronological, in order of author, etc.). This class indicates the slot in an ordered list with a fixed index.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un percorso tematico è strutturato come un collezionista di oggetti culturali o presentato con un ordine casuale o predeterminato. In quest&apos;ultimo caso, gli oggetti culturali possono essere ordinati secondo un certo criterio (ad esempio, alfabetico, cronologico, in base all&apos;autore, ecc.). Questa classe indica lo slot in una lista ordinata con un indice fisso.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Organization -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Organization">
        <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#GroupsOfPeople"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Person"/>
        <rdfs:comment xml:lang="en">A specific type of agent that corresponds to social institutions, such as companies and societies.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un tipo specifico di agente che corrisponde a istituzioni sociali, come aziende e società.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Person -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Person">
        <owl:equivalentClass rdf:resource="http://www.w3.org/2000/10/swap/pim/contact#Person"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Agent"/>
        <rdfs:comment xml:lang="en">Human individuals, regardless of whether they are alive, dead, real, or imaginary.</rdfs:comment>
        <rdfs:comment xml:lang="it">Individui umani, indipendentemente dal fatto che siano vivi, morti, reali o immaginari.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Place -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Place">
        <rdfs:comment xml:lang="it">Posizione specifica nello spazio fisico; identifica la localizzazione di qualsiasi entità. Può essere un punto preciso nello spazio (ad esempio, coordinate geografiche) o un&apos;area più ampia (ad esempio, regione geografica).</rdfs:comment>
        <rdfs:comment xml:lang="en">Specific position in physical space; identify the localization of any entity. It can be a precise point in space (i.e. geographical coordinates) or a larger area (i.e. geographical region).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Product">
        <rdfs:comment xml:lang="en">A tangible good or an intangible service offered on the market to satisfy consumers&apos; needs or desires.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un bene tangibile o un servizio intangibile offerto sul mercato per soddisfare i bisogni o desideri dei consumatori.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#RelationToAgent -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#RelationToAgent">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/prov#AgentInfluence"/>
        <rdfs:comment xml:lang="en">Class that allows linking another class to an agent playing a specific role.</rdfs:comment>
        <rdfs:comment xml:lang="it">Classe che consente di collegare un&apos;altra classe a un agente che svolge un ruolo specifico.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Role -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Role">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:comment xml:lang="it">Il ruolo svolto da un agente in un contesto specifico.</rdfs:comment>
        <rdfs:comment xml:lang="en">The role performed by an agent in a specific context.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Subject -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Subject">
        <rdfs:comment xml:lang="en">A concept or a series of concepts that concern and describe the thematic path.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un concetto o una serie di concetti che riguardano e descrivono il percorso tematico.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#TemporalEntity -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#TemporalEntity">
        <owl:equivalentClass rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Interval"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Istant"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">An abstract concept that allows the measurement of events that occur. It is a dimension in which events take place in the present, in the past and in the future.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un concetto astratto che consente la misurazione degli eventi che si verificano. È una dimensione in cui gli eventi hanno luogo nel presente, nel passato e nel futuro.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#ThematicPath -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#ThematicPath">
        <skos:Concept xml:lang="en">A thematic path is a tool for the enjoyment of cultural heritage that allows to connect cultural objects of different nature (tangible and intangible) through the identification of common themes. 
The chosen theme, which may concern specific topics, contexts, events, places, historical periods or characters and so on, is expressed synthetically by a title and in-depth with a written descriptive text; they can be further identified through subjects. 
The thematic path could be structured either as a collector of cultural objects, without establishing an order of priority, or present a predetermined order. 
The development and use of this enjoyment strategy can be profitable to convey messages and cultural, ethical and social values.</skos:Concept>
        <skos:Concept xml:lang="it">Un percorso tematico è uno strumento per  la valorizzazione del patrimonio culturale che consente di collegare oggetti culturali di diversa natura (tangibili e intangibili) attraverso l&apos;identificazione di temi comuni. Il tema scelto, che può riguardare argomenti specifici, contesti, eventi, luoghi, periodi storici o personaggi e così via, è espresso in modo sintetico da un titolo e approfondito con un testo descrittivo scritto; possono essere ulteriormente identificati attraverso soggettazioni. Il percorso tematico potrebbe essere strutturato sia come un collezionista di oggetti culturali, senza stabilire un ordine di priorità, sia presentare un ordine predefinito. Lo sviluppo e l&apos;uso di questa strategia di valorizzazione possono essere utili per trasmettere messaggi e valori culturali, etici e sociali.</skos:Concept>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Theme -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Theme">
        <rdfs:comment xml:lang="en">A broad concept or a general idea that can be explored through various subjects.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un concetto ampio o un&apos;idea generale che può essere esplorata attraverso vari argomenti.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Type -->

    <owl:Class rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Type">
        <rdfs:comment xml:lang="it">La categorizzazione di cose o entità che sono raggruppate in base a caratteristiche o attributi condivisi. I termini che caratterizzano le categorie possono essere tratti da tesauri e vocabolari controllati (ad esempio, il Getty&apos;s Art &amp; Architecture Thesaurus).</rdfs:comment>
        <rdfs:comment xml:lang="en">The categorization of things or entities that are grouped based on shared characteristics or attributes. The terms characterizing the categorizations can be taken from thesauri and controlled vocabularies (ie.e Getty&apos;s Art &amp; Architecture Thesaurus).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#Resource -->

    <owl:Class rdf:about="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    


    <!-- http://www.w3.org/2000/10/swap/pim/contact#Person -->

    <owl:Class rdf:about="http://www.w3.org/2000/10/swap/pim/contact#Person">
        <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </owl:Class>
    


    <!-- http://www.w3.org/2006/time#Instant -->

    <owl:Class rdf:about="http://www.w3.org/2006/time#Instant">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <owl:disjointWith rdf:resource="http://www.w3.org/2006/time#ProperInterval"/>
        <rdfs:comment xml:lang="en">A temporal entity with zero extent or duration</rdfs:comment>
        <rdfs:comment xml:lang="es">Una entidad temporal con una extensión o duración cero.</rdfs:comment>
        <rdfs:label xml:lang="en">Time instant</rdfs:label>
        <rdfs:label xml:lang="es">instante de tiempo.</rdfs:label>
        <skos:definition xml:lang="en">A temporal entity with zero extent or duration</skos:definition>
        <skos:definition xml:lang="es">Una entidad temporal con una extensión o duración cero.</skos:definition>
    </owl:Class>
    


    <!-- http://www.w3.org/2006/time#Interval -->

    <owl:Class rdf:about="http://www.w3.org/2006/time#Interval">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:comment xml:lang="en">A temporal entity with an extent or duration</rdfs:comment>
        <rdfs:comment xml:lang="es">Una entidad temporal con una extensión o duración.</rdfs:comment>
        <rdfs:label xml:lang="en">Time interval</rdfs:label>
        <rdfs:label xml:lang="es">intervalo de tiempo</rdfs:label>
        <skos:definition xml:lang="en">A temporal entity with an extent or duration</skos:definition>
        <skos:definition xml:lang="es">Una entidad temporal con una extensión o duración.</skos:definition>
    </owl:Class>
    


    <!-- http://www.w3.org/2006/time#ProperInterval -->

    <owl:Class rdf:about="http://www.w3.org/2006/time#ProperInterval"/>
    


    <!-- http://www.w3.org/2006/time#TemporalEntity -->

    <owl:Class rdf:about="http://www.w3.org/2006/time#TemporalEntity">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2006/time#Instant"/>
                    <rdf:Description rdf:about="http://www.w3.org/2006/time#Interval"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">A temporal interval or instant.</rdfs:comment>
        <rdfs:comment xml:lang="es">Un intervalo temporal o un instante.</rdfs:comment>
        <rdfs:label xml:lang="en">Temporal entity</rdfs:label>
        <rdfs:label xml:lang="es">entidad temporal</rdfs:label>
        <skos:definition xml:lang="en">A temporal interval or instant.</skos:definition>
