        <rdfs:domain rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Identifier"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Agent"/>
        <rdfs:comment xml:lang="it">Indica tramite quale agente pubblico o privato è stato creato il codice di identificazione della risorsa culturale.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates by which public or private agent the identification code of the cultural resource was created.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasEnd -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasEnd">
