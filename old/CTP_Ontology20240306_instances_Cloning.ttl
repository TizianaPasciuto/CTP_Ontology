#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:ObjectProperty ;
                owl:equivalentProperty ctp:hasCreator .


###  http://purl.org/dc/terms/subject
dcterms:subject rdf:type owl:ObjectProperty ;
                owl:equivalentProperty ctp:hasSubject .


###  http://purl.org/dc/terms/type
dcterms:type rdf:type owl:ObjectProperty ;
             owl:equivalentProperty ctp:hasType .


###  http://purl.org/ontology/olo/core#olo:item
olo:olo:item rdf:type owl:ObjectProperty ;
             owl:equivalentProperty ctp:hasItem ;
             rdfs:domain olo:olo:Slot ;
             rdfs:comment "An item of a slot in an ordered list."@en ;
             rdfs:isDefinedBy olo: ;
             rdfs:label "has item"@en ;
             ns:term_status "stable"@en .


###  http://purl.org/ontology/olo/core#olo:next
olo:olo:next rdf:type owl:ObjectProperty ;
             owl:equivalentProperty ctp:nextItem ;
             owl:inverseOf olo:olo:previous ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain olo:olo:Slot ;
             rdfs:range olo:olo:Slot ;
             rdfs:comment "Associates the next slot in an ordered list."@en ;
             rdfs:isDefinedBy olo: ;
             rdfs:label "has next"@en ;
             ns:term_status "testing"@en .


###  http://purl.org/ontology/olo/core#olo:ordered_list
olo:olo:ordered_list rdf:type owl:ObjectProperty ;
                     owl:equivalentProperty ctp:hasOrderedList ;
                     owl:inverseOf olo:olo:slot ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain olo:olo:Slot ;
                     rdfs:comment "An ordered list of an slot."@en ;
                     rdfs:isDefinedBy olo: ;
                     rdfs:label "has ordered list"@en ;
                     ns:term_status "testing"@en .


###  http://purl.org/ontology/olo/core#olo:previous
olo:olo:previous rdf:type owl:ObjectProperty ;
                 owl:equivalentProperty ctp:previousItem ;
                 rdf:type owl:InverseFunctionalProperty ;
                 rdfs:domain olo:olo:Slot ;
                 rdfs:range olo:olo:Slot ;
                 rdfs:comment "Associates the previous slot in an ordered list"@en ;
                 rdfs:isDefinedBy olo: ;
                 rdfs:label "has previous"@en ;
                 ns:term_status "testing"@en .


###  http://purl.org/ontology/olo/core#olo:slot
olo:olo:slot rdf:type owl:ObjectProperty ;
             owl:equivalentProperty ctp:hasSlot ;
             rdfs:range olo:olo:Slot ;
             rdfs:comment "A slot in an ordered list."@en ;
             rdfs:isDefinedBy olo: ;
             rdfs:label "has slot"@en ;
             ns:term_status "stable"@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#allowsTheSaleOf
ctp:allowsTheSaleOf rdf:type owl:ObjectProperty ;
                    rdfs:domain ctp:ThematicPath ;
                    rdfs:range ctp:Product ;
                    rdfs:comment "Indica una risorsa collegata al percorso tematico che può essere venduta durante il suo utilizzo (ad esempio, il catalogo di una mostra descritta dal percorso tematico)."@it ,
                                 "Indicates a resource linked to the thematic path that can be sold during its use (i.e. the catalog of an exhibition described by the thematic path)."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasBeginning
ctp:hasBeginning rdf:type owl:ObjectProperty ;
                 owl:equivalentProperty <http://www.w3.org/2006/time#hasBeginning> ;
                 rdfs:subPropertyOf ctp:hasTime ;
                 rdfs:domain ctp:TemporalEntity ;
                 rdfs:range ctp:Istant ;
                 rdfs:comment "Beginning of a temporal entity."@en ,
                              "Inizio di un'entità temporale."@it .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasCreator
ctp:hasCreator rdf:type owl:ObjectProperty ;
               rdfs:domain ctp:Identifier ;
               rdfs:range ctp:Agent ;
               rdfs:comment "Indica tramite quale agente pubblico o privato è stato creato il codice di identificazione della risorsa culturale."@it ,
                            "Indicates by which public or private agent the identification code of the cultural resource was created."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasEnd
ctp:hasEnd rdf:type owl:ObjectProperty ;
           owl:equivalentProperty <http://www.w3.org/2006/time#hasEnd> ;
           rdfs:subPropertyOf ctp:hasTime ;
           rdfs:domain ctp:TemporalEntity ;
           rdfs:range ctp:Istant ;
           rdfs:comment "End of a temporal entity."@en ,
                        "Fine di una entità temporale."@it .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasGeometry
ctp:hasGeometry rdf:type owl:ObjectProperty ;
                owl:equivalentProperty locn:locn:geometry ;
                rdfs:domain ctp:Place ;
                rdfs:range ctp:Geometry ;
                rdfs:comment "Associa un oggetto spaziale con una geometria corrispondente."@it ,
                             "Associates a spatial thing with a corresponding geometry."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasItem
ctp:hasItem rdf:type owl:ObjectProperty ;
            rdfs:domain ctp:OrderedListSlot ;
            rdfs:range ctp:CulturalResource ;
            rdfs:comment "Indica una risorsa culturale in una lista ordinata."@it ,
                         "Indicates a slot cultural resource in an ordered list."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasLocation
ctp:hasLocation rdf:type owl:ObjectProperty ;
                owl:inverseOf ctp:isLocationOf ;
                rdfs:domain owl:Thing ;
                rdfs:range ctp:Place ;
                rdfs:comment "Indica la posizione dell'entità. Nel caso di una CulturalResource, ci si riferisce a risorse immobili (ad esempio, beni paesaggistici, edifici storici, chiese, ecc.)."@it ,
                             "Indicates the location of the entity. In the case of CulturalResource, it's referred to immovable resources (i.e. landscape assets, historical buildings, churches, etc.)."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasMember
ctp:hasMember rdf:type owl:ObjectProperty ;
              owl:inverseOf ctp:memberOf ;
              rdfs:domain ctp:GroupsOfPeople ;
              rdfs:range ctp:Agent ;
              rdfs:comment "Indica che un gruppo (inteso come semplice raggruppamento di persone o come società) ha agenti come membri."@it ,
                           "Indicates that a group (intended as a simple grouping of people or as a company) has agents as members."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasOrderedList
ctp:hasOrderedList rdf:type owl:ObjectProperty ;
                   owl:inverseOf ctp:hasSlot ;
                   rdfs:domain ctp:OrderedListSlot ;
                   rdfs:range ctp:ThematicPath ;
                   rdfs:comment "Indica come sono organizzati gli slot del percorso tematico."@it ,
                                "It indicates how the slots of the thematic path are organized."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasPart
ctp:hasPart rdf:type owl:ObjectProperty ;
            owl:inverseOf ctp:isPartOf ;
            rdfs:domain ctp:ThematicPath ;
            rdfs:range ctp:CulturalResource ;
            owl:propertyChainAxiom ( ctp:hasSlot
                                     ctp:hasItem
                                   ) ;
            rdfs:comment "Indica la relazione tra il percorso tematico e le risorse culturali che lo compongono."@it ,
                         "Indicates the relationship between the thematic path and the cultural resources that compose it."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasPlace
ctp:hasPlace rdf:type owl:ObjectProperty ;
             rdfs:domain ctp:Agent ;
             rdfs:range ctp:Place ;
             rdfs:comment "Indica la localizzazione di un agente."@it ,
                          "Indicates the location of an agent."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasRelationToAgent
ctp:hasRelationToAgent rdf:type owl:ObjectProperty ;
                       rdfs:domain owl:Thing ;
                       rdfs:range ctp:RelationToAgent ;
                       rdfs:comment "Indica la relazione tra un'entità e un agente coinvolto con un determinato ruolo."@it ,
                                    "Indicates the relationship between an entity and an agent involved with a certain role."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasRole
ctp:hasRole rdf:type owl:ObjectProperty ;
            owl:equivalentProperty prov:hadRole ;
            rdfs:domain ctp:RelationToAgent ;
            rdfs:range ctp:Role ;
            rdfs:comment "Indica il tipo di ruolo svolto dall'agente (ad esempio creatore, finanziatore, ecc.)."@it ,
                         "Indicates the type of role played by the agent (i.e. creator, funder, etc)."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasSecondaryIdentifier
ctp:hasSecondaryIdentifier rdf:type owl:ObjectProperty ;
                           owl:equivalentProperty adms:adms:identifier ;
                           rdfs:domain owl:Thing ;
                           rdfs:range ctp:Identifier ;
                           rdfs:comment "Indica la relazione tra un'entità e il codice di identificazione assegnato ad essa da un agente che la gestisce."@it ,
                                        "Indicates the relationship between an entity and the identification code assigned to it by an agent that manages it."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasSlot
ctp:hasSlot rdf:type owl:ObjectProperty ;
            rdfs:domain ctp:ThematicPath ;
            rdfs:range ctp:OrderedListSlot ;
            rdfs:comment "Per consentire la classificazione delle risorse culturali secondo un determinato criterio, il percorso tematico è organizzato attraverso slot che possono essere indicizzati."@it ,
                         "To allow the sorting of cultural resources according to a certain criterion, the thematic path is organized through slots that can be indexed."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasSubject
ctp:hasSubject rdf:type owl:ObjectProperty ;
               rdfs:domain owl:Thing ;
               rdfs:range ctp:Subject ;
               rdfs:comment "Indica a quale/i soggetto/i l'entità si riferisce."@it ,
                            "Indicates to which subject/subjects the entity refers/refer."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasTheme
ctp:hasTheme rdf:type owl:ObjectProperty ;
             rdfs:domain ctp:ThematicPath ;
             rdfs:range ctp:Theme ;
             rdfs:comment "Indica il tema su cui si basa il percorso tematico."@it ,
                          "Indicates the theme on which the thematic path is based."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasTime
ctp:hasTime rdf:type owl:ObjectProperty ;
            owl:equivalentProperty <http://www.w3.org/2006/time#hasTime> ;
            rdfs:domain owl:Thing ;
            rdfs:range ctp:TemporalEntity ;
            rdfs:comment "Indica il momento in cui un'entità è stata creata e/o un evento è avvenuto."@it ,
                         "Indicates the time an entity was created and/or an event occurred."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#hasType
ctp:hasType rdf:type owl:ObjectProperty ;
            rdfs:domain owl:Thing ;
            rdfs:range ctp:Type ;
            rdfs:comment "Indica il tipo di entità."@it ,
                         "Indicates the type of entity."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#includes
ctp:includes rdf:type owl:ObjectProperty ;
             owl:inverseOf ctp:isIncludedIn ;
             rdfs:domain ctp:ThematicPath ;
             rdfs:range ctp:ThematicPath ;
             rdfs:comment "Indica che un percorso tematico può includere sottopercorsi tematici. Pertanto, possono essere utilizzati come approfondimento degli argomenti trattati precedentemente."@it ,
                          "Indicates that a thematic path may include sub-thematic paths. Therefore, they can be used as a deepening of the topics dealt with previously."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#involves
ctp:involves rdf:type owl:ObjectProperty ;
             rdfs:domain ctp:RelationToAgent ;
             rdfs:range ctp:Agent ;
             rdfs:comment "Indica quale agente è coinvolto in una relazione con un certo ruolo."@it ,
                          "Indicates which agent is involved in a relationship with a certain role."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#isIncludedIn
ctp:isIncludedIn rdf:type owl:ObjectProperty ;
                 rdfs:domain ctp:ThematicPath ;
                 rdfs:range ctp:ThematicPath ;
                 rdfs:comment "Indica che un sottopercorso tematico è incluso in un percorso tematico."@it ,
                              "Indicates that a sub-thematic path is included in a thematic path."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#isInspiredByAgent
ctp:isInspiredByAgent rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf ctp:hasSubject ;
                      rdfs:domain ctp:ThematicPath ;
                      rdfs:range ctp:Agent ;
                      rdfs:comment "Indica la relazione tra il percorso tematico e l'agente che lo ispira."@it ,
                                   "Indicates the relationship between the thematic path and the agent that inspires it."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#isLocationOf
ctp:isLocationOf rdf:type owl:ObjectProperty ;
                 rdfs:domain ctp:Place ;
                 rdfs:range owl:Thing ;
                 rdfs:comment "Indica l'associazione tra una posizione e l'entità che vi è localizzata."@it ,
                              "Indicates the association between a location and the entity that is located there."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#isPartOf
ctp:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:domain ctp:CulturalResource ;
             rdfs:range ctp:ThematicPath ;
             rdfs:comment "Indica la relazione tra l'oggetto culturale e il/i percorso/percorsi tematico/i a cui è collegato."@it ,
                          "Indicates the relationship between the cultural object and the thematic path/paths to which it is linked."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#isPreservedBy
ctp:isPreservedBy rdf:type owl:ObjectProperty ;
                  rdfs:domain ctp:CulturalResource ;
                  rdfs:comment "Indica l'agente pubblico o privato responsabile della conservazione della risorsa culturale."@it ,
                               "Indicates the public or private agent responsible for preserving the cultural resource."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#isRealizedIn
ctp:isRealizedIn rdf:type owl:ObjectProperty ;
                 rdfs:domain ctp:ThematicPath ;
                 rdfs:range ctp:CulturalEvent ;
                 rdfs:comment "Indica la relazione tra un percorso tematico e un evento culturale che lo origina (ad esempio, una mostra fisica)."@it ,
                              "Indicates the relationship between a thematic path and a cultural event that originates it (i.e. physical exhibition)."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#memberOf
ctp:memberOf rdf:type owl:ObjectProperty ;
             rdfs:domain ctp:Agent ;
             rdfs:range ctp:GroupsOfPeople ;
             rdfs:comment "Indica che un agente fa parte di un gruppo, inteso come semplice raggruppamento di persone o come società."@it ,
                          "Indicates that an agent is part of a group, intended as a simple grouping of people or as a company."@en .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#nextItem
ctp:nextItem rdf:type owl:ObjectProperty ;
             owl:inverseOf ctp:previousItem ;
             rdfs:domain ctp:OrderedListSlot ;
             rdfs:range ctp:OrderedListSlot ;
             rdfs:comment "In an ordered collection, indicates the link to the next item in a list."@en ,
                          "In un insieme ordinato, indica il collegamento all'elemento successivo in un elenco."@it .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#previousItem
ctp:previousItem rdf:type owl:ObjectProperty ;
                 rdfs:domain ctp:OrderedListSlot ;
                 rdfs:range ctp:OrderedListSlot ;
                 rdfs:comment "In an ordered collection, indicates the link to the previous item in a list."@en ,
                              "In un insieme ordinato, indica il collegamento all'elemento precedente in un elenco."@it .


###  http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#refersToPlace
