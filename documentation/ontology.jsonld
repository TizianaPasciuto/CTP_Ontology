}, {
  "@id" : "http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Anatomy",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Subject"
