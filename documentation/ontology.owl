    <!-- https://w3id.org/ctp#IsInvolvedIn -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#IsInvolvedIn">
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#involves"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#RelationToAgent"/>
        <rdfs:comment xml:lang="it">Indica l&apos;agente coinvolto in una relazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the agent involved in a relation.</rdfs:comment>
        <rdfs:label xml:lang="en">isInvolvedIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#allowsTheSaleOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#allowsTheSaleOf">
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#soldThrough"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#ThematicPath"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#Product"/>
        <rdfs:comment xml:lang="it">Indica una risorsa collegata al percorso tematico che può essere venduta durante il suo utilizzo (ad esempio, il catalogo di una mostra descritta dal percorso tematico).</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates a resource linked to the thematic path that can be sold during its use (i.e. the catalog of an exhibition described by the thematic path).</rdfs:comment>
        <rdfs:label xml:lang="en">allowsTheSaleOf</rdfs:label>
        <skos:editorialNote xml:lang="en">Preferred direction of the relationship.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#hasBeginning -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#hasBeginning">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ctp#hasTime"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#isBeginningOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#TemporalEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#Instant"/>
        <rdfs:comment xml:lang="en">Beginning of a temporal entity.</rdfs:comment>
        <rdfs:comment xml:lang="it">Inizio di un&apos;entità temporale.</rdfs:comment>
        <rdfs:label xml:lang="en">hasBeginning</rdfs:label>
        <skos:editorialNote xml:lang="en">Preferred direction of the relationship.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#hasCreator -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#hasCreator">
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#isCreatorOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#Identifier"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#Agent"/>
        <rdfs:comment xml:lang="it">Indica tramite quale agente pubblico o privato è stato creato il codice di identificazione della risorsa culturale.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates by which public or private agent the identification code of the cultural resource was created.</rdfs:comment>
        <rdfs:label xml:lang="en">hasCreator</rdfs:label>
        <skos:editorialNote xml:lang="en">Preferred direction of the relationship.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#hasEnd -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#hasEnd">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ctp#hasTime"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#isEndOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#TemporalEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#Instant"/>
        <rdfs:comment xml:lang="en">End of a temporal entity.</rdfs:comment>
        <rdfs:comment xml:lang="it">Fine di una entità temporale.</rdfs:comment>
        <rdfs:label xml:lang="en">hasEnd</rdfs:label>
        <skos:editorialNote xml:lang="en">Preferred direction of the relationship.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#hasGeometry -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#hasGeometry">
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#isGeometryOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#Place"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#Geometry"/>
        <rdfs:comment xml:lang="it">Associa un oggetto spaziale con una geometria corrispondente.</rdfs:comment>
        <rdfs:comment xml:lang="en">Associates a spatial thing with a corresponding geometry.</rdfs:comment>
        <rdfs:label xml:lang="en">hasGeometry</rdfs:label>
        <skos:editorialNote xml:lang="en">Preferred direction of the relationship.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#hasItem -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#hasItem">
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#isItemOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#OrderedListSlot"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#CulturalResource"/>
        <rdfs:comment xml:lang="it">Indica una risorsa culturale in una lista ordinata.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates a slot cultural resource in an ordered list.</rdfs:comment>
        <rdfs:label xml:lang="en">hasItem</rdfs:label>
        <skos:editorialNote xml:lang="en">Preferred direction of the relationship.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#hasLocation -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#hasLocation">
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#isLocationOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#Place"/>
        <rdfs:comment xml:lang="it">Indica la posizione dell&apos;entità. Nel caso di una CulturalResource, ci si riferisce a risorse immobili (ad esempio, beni paesaggistici, edifici storici, chiese, ecc.).</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the location of the entity. In the case of CulturalResource, it&apos;s referred to immovable resources (i.e. landscape assets, historical buildings, churches, etc.).</rdfs:comment>
        <rdfs:label xml:lang="en">hasLocation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#hasMember -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#hasMember">
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#memberOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#GroupOfPeople"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#Agent"/>
        <rdfs:comment xml:lang="it">Indica che un gruppo (inteso come semplice raggruppamento di persone o come società) ha agenti come membri.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates that a group (intended as a simple grouping of people or as a company) has agents as members.</rdfs:comment>
        <rdfs:label xml:lang="en">hasMember</rdfs:label>
    </owl:ObjectProperty>
    


