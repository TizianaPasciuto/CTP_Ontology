        <rdfs:comment xml:lang="en">Indicates the link that allows viewing the entity available on the web through an hyperlink or a QR code.</rdfs:comment>
        <rdfs:label xml:lang="en">linkToResource</rdfs:label>
    </owl:DatatypeProperty>
    


