        <rdfs:comment xml:lang="en">Indicates that an agent is part of a group, intended as a simple grouping of people or as a company.</rdfs:comment>
        <rdfs:label xml:lang="en">memberOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#nextItem -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#nextItem">
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#previousItem"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#OrderedListSlot"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#OrderedListSlot"/>
        <rdfs:comment xml:lang="en">In an ordered collection, indicates the link to the next item in a list.</rdfs:comment>
        <rdfs:comment xml:lang="it">In un insieme ordinato, indica il collegamento all&apos;elemento successivo in un elenco.</rdfs:comment>
        <rdfs:label xml:lang="en">nextItem</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#occursIn -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#occursIn">
        <rdfs:domain rdf:resource="https://w3id.org/ctp#CulturalEvent"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#ThematicPath"/>
