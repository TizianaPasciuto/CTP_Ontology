    <!-- https://w3id.org/ctp#isTypeOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#isTypeOf">
        <rdfs:domain rdf:resource="https://w3id.org/ctp#Type"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="it">Indica di che tipologia è l&apos;entità.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates what type the entity belongs to.</rdfs:comment>
        <rdfs:label xml:lang="en">isTypeOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#memberOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#memberOf">
        <rdfs:domain rdf:resource="https://w3id.org/ctp#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#GroupOfPeople"/>
