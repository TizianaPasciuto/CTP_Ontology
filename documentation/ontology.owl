        <rdfs:comment xml:lang="en">Indicates the relationship between a thematic path and a cultural event that originates it (i.e. physical exhibition).</rdfs:comment>
        <rdfs:label xml:lang="en">occursIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#placeRefersTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#placeRefersTo">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ctp#isSubjectOf"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#refersToPlace"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#Place"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#ThematicPath"/>
        <rdfs:comment xml:lang="it">Indica la relazione tra il percorso tematico e i luoghi a cui è collegato.</rdfs:comment>
        <rdfs:comment xml:lang="en">It indicates the relationship between the thematic path and the places to which it is linked.</rdfs:comment>
        <rdfs:label xml:lang="en">placeRefersTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#preserves -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#preserves">
        <rdfs:domain rdf:resource="https://w3id.org/ctp#RelationToAgent"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#CulturalResource"/>
        <rdfs:comment xml:lang="it">Indica l&apos;agente pubblico o privato responsabile della conservazione della risorsa culturale.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the public or private agent responsible for preserving the cultural resource.</rdfs:comment>
        <rdfs:label xml:lang="en">preserves</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#previousItem -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#previousItem">
        <rdfs:domain rdf:resource="https://w3id.org/ctp#OrderedListSlot"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#OrderedListSlot"/>
        <rdfs:comment xml:lang="en">In an ordered collection, indicates the link to the previous item in a list.</rdfs:comment>
        <rdfs:comment xml:lang="it">In un insieme ordinato, indica il collegamento all&apos;elemento precedente in un elenco.</rdfs:comment>
        <rdfs:label xml:lang="en">previousItem</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#refersToPlace -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#refersToPlace">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ctp#hasSubject"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#ThematicPath"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#Place"/>
        <rdfs:comment xml:lang="it">Indica la relazione tra il percorso tematico e i luoghi a cui è collegato.</rdfs:comment>
        <rdfs:comment xml:lang="en">It indicates the relationship between the thematic path and the places to which it is linked.</rdfs:comment>
        <rdfs:label xml:lang="en">refersToPlace</rdfs:label>
        <skos:editorialNote xml:lang="en">Preferred direction of the relationship.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#refersToTemporalEntity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#refersToTemporalEntity">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ctp#hasSubject"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ctp#temporalEntityRefersTo"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#ThematicPath"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#TemporalEntity"/>
        <rdfs:comment xml:lang="it">Indica la relazione tra il percorso tematico e il periodo storico a cui si riferisce.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the relationship between the thematic path and the historical period to which it refers.</rdfs:comment>
        <rdfs:label xml:lang="en">refersToTemporalEntity</rdfs:label>
        <skos:editorialNote xml:lang="en">Preferred direction of the relationship.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#soldThrough -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#soldThrough">
        <rdfs:domain rdf:resource="https://w3id.org/ctp#Product"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#ThematicPath"/>
        <rdfs:comment xml:lang="it">Indica una risorsa collegata al percorso tematico che può essere venduta durante il suo utilizzo (ad esempio, il catalogo di una mostra descritta dal percorso tematico).</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates a resource linked to the thematic path that can be sold during its use (i.e. the catalog of an exhibition described by the thematic path).</rdfs:comment>
        <rdfs:label xml:lang="en">soldThrough</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#takesPlaceIn -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#takesPlaceIn">
        <rdfs:domain rdf:resource="https://w3id.org/ctp#CulturalEvent"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#Place"/>
        <rdfs:comment xml:lang="it">Indica la relazione tra un evento culturale e il luogo in cui si è svolto.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the relationship between a cultural event and the place where it took place.</rdfs:comment>
        <rdfs:label xml:lang="en">takePlaceIn</rdfs:label>
        <skos:editorialNote xml:lang="en">Preferred direction of the relationship.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ctp#temporalEntityRefersTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ctp#temporalEntityRefersTo">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ctp#isSubjectOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#TemporalEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/ctp#ThematicPath"/>
        <rdfs:comment xml:lang="it">Indica la relazione tra il percorso tematico e il periodo storico a cui si riferisce.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the relationship between the thematic path and the historical period to which it refers.</rdfs:comment>
        <rdfs:label xml:lang="en">temporalEntityRefersTo</rdfs:label>
    </owl:ObjectProperty>
    


