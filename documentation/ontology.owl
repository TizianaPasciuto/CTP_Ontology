        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Indica una descrizione di un&apos;entità.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates a description of an entity.</rdfs:comment>
        <rdfs:label>hasDescription</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ctp#hasIndex -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ctp#hasIndex">
