        <rdfs:comment xml:lang="en">Indicates a date or a chronological range that characterizes an entity.</rdfs:comment>
        <rdfs:label xml:lang="en">hasDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ctp#hasDescription -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ctp#hasDescription">
