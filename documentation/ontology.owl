        <rdfs:comment xml:lang="en">Indicates how cultural resources are indexed within the thematic path, so they appear ordered according to a specific criterion (i.e. alphabetical, chronological, etc).</rdfs:comment>
        <rdfs:label xml:lang="en">hasIndex</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ctp#hasName -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ctp#hasName">
        <rdfs:domain rdf:resource="https://w3id.org/ctp#Agent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Indica il nome associato a un agente.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the name associated with an agent.</rdfs:comment>
        <rdfs:label xml:lang="en">hasName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ctp#hasNotation -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ctp#hasNotation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ctp#Identifier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Indica il codice di identificazione associato all&apos;entità.</rdfs:comment>
        <rdfs:comment xml:lang="en">The string that constitutes the cultural resource identifier.</rdfs:comment>
        <rdfs:label xml:lang="en">hasNotation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ctp#hasPrimaryIdentifier -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ctp#hasPrimaryIdentifier">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Indica un identificatore univoco della risorsa.</rdfs:comment>
        <rdfs:comment xml:lang="en">It indicates a unique identifier of the entity.</rdfs:comment>
        <rdfs:label>hasPrimaryIdentifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ctp#hasTitle -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ctp#hasTitle">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Indica il titolo di un&apos;entità.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the title of an entity.</rdfs:comment>
        <rdfs:label xml:lang="en">hasTitle</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ctp#isSelledBy -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ctp#isSelledBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
