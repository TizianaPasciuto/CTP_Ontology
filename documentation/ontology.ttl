:Double_barometer rdf:type owl:NamedIndividual ,
                           :CulturalResource ;
                  :hasRelationToAgent :Costructor_1 ,
                                      :Creator_2 ;
                  :hasSecondaryIdentifier <http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#IT/ItRC/PST/00000001> ;
                  :hasTime <http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#1751/1800> ;
