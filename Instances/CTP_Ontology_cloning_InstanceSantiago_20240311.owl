    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Road_to_Santiago_de_Compostela">
        <rdf:type rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#ThematicPath"/>
        <hasRelationToAgent rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Certification_body_1"/>
