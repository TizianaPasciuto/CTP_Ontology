        <hasSlot rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Index02"/>
        <hasSlot rdf:resource="http://www.semanticweb.org/principale/ontologies/2024/0/CTP_Ontology#Index03"/>
