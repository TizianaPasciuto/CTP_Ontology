###  https://w3id.org/ctp#conveys
ctp:conveys rdf:type owl:DatatypeProperty ;
            rdfs:domain ctp:ThematicPath ;
            rdfs:range rdfs:Literal ;
            rdfs:comment "Indica un messaggio che può trasmettere valori culturali, etici e sociali attraverso il percorso tematico."@it ,
                         "Indicates a message that can convey cultural, ethical and social values through the thematic path."@en ;
            rdfs:label "conveys"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource ctp:conveys ;
   owl:annotatedProperty rdfs:range ;
   owl:annotatedTarget rdfs:Literal ;
   rdfs:comment "meaning"
 ] .


